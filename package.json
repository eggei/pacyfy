{
  "name": "pacyfy",
  "version": "0.0.2",
  "description": "CLI for orchestrating Cypress tests",
  "author": "eggei",
  "bin": {
    "pacyfy": "./bin/run"
  },
  "homepage": "https://github.com/eggei/pacyfy",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eggei/pacyfy",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-commands": "^2.2.15",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@types/wait-on": "^5.3.1",
    "fastify": "^4.19.2",
    "listr2": "^6.6.1",
    "rxjs": "^7.8.1",
    "tree-kill": "^1.2.2",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.20",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.31",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "pacyfy",
    "dirname": "pacyfy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "hooks": {
      "init": "./dist/hooks/init/printLogo"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "dev-pacyfy": "cd test/test-app/ui && ../../../bin/dev"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/eggei/pacyfy/issues",
  "keywords": [
    "cypress",
    "testing",
    "cli",
    "orchestrating",
    "orchestration",
    "automation",
    "oclif",
    "ci",
    "parallel",
    "server",
    "start",
    "test",
    "utility",
    "wait",
    "start-server-and-test"
  ],
  "types": "dist/index.d.ts"
}
